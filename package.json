{
  "name": "mongodb-pipeline-factory",
  "version": "0.22.0",
  "description": "Generate MongoDB pipelines with ease. Save dozens of keystrokes.",
  "keywords": [
    "mongodb",
    "pipeline",
    "node",
    "mongoose",
    "generator",
    "factory"
  ],
  "main": "dist/bundle.js",
  "scripts": {
    "dev": "webpack --progress --colors --watch --env dev",
    "docs": "./node_modules/.bin/typedoc --target ES6 --hideGenerator --out ./docs src/",
    "build": "webpack --env dev && webpack --env build",
    "test": "mocha --require ts-node/register ts,tsx 'test/**/*.{js,ts,tsx}'",
    "test:watch": "mocha --require ts-node/register --watch-extensions ts,tsx -w 'test/**/*.{js,ts,tsx}'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ndabAP/mongodb-pipeline-factory"
  },
  "author": "Julian Claus <kontakt@julian-claus.de>",
  "license": "MIT",
  "devDependencies": {
    "@types/lodash": "^4.14.109",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
    "ts-node": "^6.1.0",
    "ts-loader": "^4.3.1",
    "typedoc": "^0.14.2",
    "typescript": "^2.9.1",
    "webpack": "^4.10.2",
    "webpack-cli": "^2.1.3"
  },
  "dependencies": {}
}
